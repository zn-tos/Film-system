\n    };\r\n  },\r\n  created() {\r\n    const _this = this;\r\n    axios.get(\"http://localhost:8181/film/getFilms\").then(function (resp) {\r\n      console.log(resp.data);\r\n      _this.$data.movies = resp.data.list;\r\n\r\n      //_this.tableData = resp.data.list;\r\n      // _this.total = resp.data.total;\r\n      // _this.size = resp.data.pageSize;\r\n    });\r\n    //this.findAll();\r\n    //this.findSortName();\r\n  },\r\n  methods: {\r\n    /*  addTo(id){\r\n      let token = ''\r\n      try {\r\n        token= JSON.parse(sessionStorage.userInfo).token\r\n      } catch (error) {\r\n        token = ''\r\n      }\r\n      if(token){\r\n          axios.post(\"http://localhost:8082/mai/addTocart\",{username:this.$store.state.userInfo.user.username,\r\n          movie_id:id},{\r\n            headers: {\r\n          'Content-Type':'application/json; charset=UTF-8'//设置传输JSON格式即可\r\n        }}\r\n        )\r\n            .then(resp=> {\r\n              let code=resp.data.code;\r\n              if(code===200){\r\n                this.$message({\r\n                  type: \"success\",\r\n                  message: \"成功添加进购物车\"\r\n                });\r\n              }\r\n              else {\r\n                this.$message({\r\n                  type: \"fail\",\r\n                  message: \"添加失败\"\r\n                });\r\n              }}\r\n            )\r\n      }\r\n      else{\r\n        this.$message({\r\n              type: \"fail\",\r\n              message: \"请先登录\"\r\n          }\r\n        );\r\n        this.$router.push('/login')\r\n      }\r\n    },\r\n    findAll(){\r\n      axios\r\n        .get(\"http://localhost:8082/mai/findAll\",{\r\n          headers:{\r\n            'Content-type':'application/x-www-form-urlencoded;charset=UTF-8'\r\n          }\r\n        })\r\n        .then(resp=>{\r\n          this.$data.movie=resp.data.data;\r\n        });\r\n    },\r\n    findSortName(){\r\n      axios\r\n        .get(\"http://localhost:8082/mai/findSortName\",{\r\n          headers:{\r\n            'Content-type':'application/x-www-form-urlencoded;charset=UTF-8'\r\n          }\r\n        })\r\n        .then(resp=>{\r\n          this.$data.sortList=resp.data.data;\r\n        });\r\n    },\r\n    findmovie(){\r\n      axios\r\n        .post(\"http://localhost:8082/mai/findByName\",this.input,{\r\n          headers:{\r\n            'Content-type':'application/x-www-form-urlencoded;charset=UTF-8'\r\n          }\r\n        })\r\n        .then(resp=> {\r\n          let code=resp.data.code;\r\n          if(code===200){\r\n            this.$data.movie=resp.data.data;\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"相关书籍如下\"\r\n            })\r\n          }\r\n          else{\r\n            this.$message({\r\n              type: \"fail\",\r\n              message: \"没找到相关的信息\"\r\n            });\r\n          }\r\n          }\r\n        )\r\n    },\r\n    findBySort(ite){\r\n      axios\r\n        .post(\"http://localhost:8082/mai/findBySort\",ite,{\r\n        headers:{\r\n          'Content-type':'application/x-www-form-urlencoded;charset=UTF-8'\r\n        }\r\n      })\r\n        .then(resp=> {\r\n            let code=resp.data.code;\r\n            if(code===200){\r\n              this.$data.movie=resp.data.data;\r\n              this.$message({\r\n                type: \"success\",\r\n                message: \"相关书籍如下\"\r\n              })\r\n            }\r\n            else{\r\n              this.$message({\r\n                type: \"fail\",\r\n                message: \"没找到相关的信息\"\r\n              });\r\n            }\r\n          }\r\n        )\r\n    }*/\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.el-container {\r\n  height: 100%;\r\n}\r\n.el-aside {\r\n  color: #333;\r\n}\r\n</style>\r\n"]}]}