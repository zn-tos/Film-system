AAA;AAFA,OAAA,EADA,CAKA;AACA;AACA,KAhMA;AAiMA,IAAA,kBAjMA,8BAiMA,IAjMA,EAiMA;AACA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,GAAA,CAAA,CAFA,CAIA;AACA;AACA;;AACA,UAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,mBAAA;AACA;;AACA,aAAA,MAAA;AACA,KA5MA;AA6MA,IAAA,gBA7MA,4BA6MA,IA7MA,EA6MA,QA7MA,EA6MA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA;AA/MA;AA3GA,CAAA","sourcesContent":["<template>\r\n  <div >\r\n    <div >\r\n      <el-menu\r\n        :default-active=\"this.$route.path\"\r\n        class=\"el-menu-demo\"\r\n        mode=\"horizontal\"\r\n        background-color=\"#545c64\"\r\n        text-color=\"#fff\"\r\n        active-text-color=\"#ffd04b\"\r\n      >\r\n        <el-menu-item index=\"1\">电影信息</el-menu-item>\r\n      </el-menu>\r\n      <br />\r\n      <!-- 点击触发add方法 -->\r\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\r\n      <el-table :data=\"tableData\" style=\"text-align:center;width: 100%\">\r\n        <!--        <el-table-column prop=\"id\" label=\"序号\" width=\"180\"></el-table-columntype=\"index\">-->\r\n        <el-table-column prop=\"name\" label=\"电影名称\"  width=\"160\" ></el-table-column>\r\n        <el-table-column prop=\"id\" label=\"电影编号\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"birthday\" label=\"时间\" width=\"90\"></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"时长\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"lang\" label=\"语言\" width=\"120\"></el-table-column>\r\n\r\n        <el-table-column prop=\"country\" label=\"国家\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"brief\" label=\"简介\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"score\" label=\"评分\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"pic\" label=\"图片\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"cate\" label=\"分类\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"campany\" label=\"出品公司\" width=\"100\"></el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            <!-- 点击编辑进入编辑页面进行编辑表格数据 -->\r\n            <el-button size=\"small\" @click=\"handleEdit(scope.$index, scope.row)\">编辑</el-button>\r\n            <el-button size=\"small\" type=\"danger\" @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n\r\n    <!-- 下面这个用来设置点击添加按钮的弹出框，里面可以进行嵌套表格来展示弹出的表格信息,使用下面的:visible.sync来控制显示与否 -->\r\n    <!-- 里面绑定的是我们新设置的值，填写完成后，将我们这个新值塞到页面中所有的数据当中去 -->\r\n    <el-dialog title=\"出版社信息\" :visible.sync=\"dialogFormVisible\">\r\n      <!-- 在el-dialog中进行嵌套el-form实现弹出表格的效果 -->\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"form\">\r\n        <el-form-item label=\"书籍编号(不允许重复)\" prop=\"book_id\" :label-width=\"formLabelWidth\" >\r\n          <el-input v-if=\"mark>0\" v-model=\"form.book_id\" auto-complete=\"off\" ></el-input>\r\n          <el-input v-else v-model=\"form.book_id\" auto-complete=\"off\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"书籍名称\" prop=\"book_name\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.book_name\" auto-complete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\" prop=\"price\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.price\" auto-complete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出版社\" prop=\"publisher\" :label-width=\"formLabelWidth\">\r\n          <el-select v-model=\"form.publisher\" placeholder=\"请选择\" >\r\n            <el-option\r\n              v-for=\"item in publisher\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.name\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"日期(格式:YY-MM-DD)\" prop=\"date\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.date\" auto-complete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图片(必填)\" prop=\"image\" :label-width=\"formLabelWidth\">\r\n          <el-upload\r\n            class=\"avatar-uploader\"\r\n            action=\"http://localhost:8083/upload\"\r\n            :show-file-list=\"false\"\r\n            :on-remove=\"handleAvatarFail\"\r\n            :on-success=\"handleAvatarSuccess\"\r\n            :before-upload=\"beforeAvatarUpload\">\r\n            <img v-if=\"form.image\" :src=\"require(`../assets/${form.image}`)\" class=\"avatar\">\r\n            <i class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n<!--          <el-input v-model=\"form.image\" auto-complete=\"off\"></el-input>-->\r\n        </el-form-item>\r\n<!--        <el-form-item label=\"大类\" prop=\"big_class\" :label-width=\"formLabelWidth\">-->\r\n<!--          <el-input v-model=\"form.big_class\" auto-complete=\"off\"></el-input>-->\r\n<!--        </el-form-item>-->\r\n        <el-form-item label=\"大类(先选择大类再选小类)\"  prop=\"big_class\" :label-width=\"formLabelWidth\" >\r\n          <el-select v-model=\"form.big_class\" placeholder=\"请选择\"  @change=\"getOption2(form.big_class)\">\r\n            <el-option\r\n              v-for=\"item in option1\"\r\n              :key=\"item.id\"\r\n              :label=\"item.big\"\r\n              :value=\"item.big\">\r\n            </el-option>\r\n          </el-select>\r\n\r\n        </el-form-item>\r\n        <el-form-item label=\"小类\" prop=\"small_class\" :label-width=\"formLabelWidth\">\r\n          <el-select v-model=\"form.small_class\" placeholder=\"请选择\" >\r\n            <el-option\r\n              v-for=\"item in option2\"\r\n              :key=\"item.index\"\r\n              :label=\"item.small\"\r\n              :value=\"item.small\">\r\n            </el-option>\r\n          </el-select>\r\n<!--          <el-input v-model=\"form.small_class\" auto-complete=\"off\"></el-input>-->\r\n        </el-form-item>\r\n        <el-form-item label=\"作者\" prop=\"author\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.author\" auto-complete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"译者\" prop=\"translator\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.translator\" auto-complete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item style=\"text-align: right\">\r\n          <el-button @click=\"cancel\">取 消</el-button>\r\n          <!-- 设置触发更新的方法 -->\r\n          <el-button type=\"primary\" @click=\"update('form')\">确 定</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script type=\"text/ecmascript-6\">\r\nexport default {\r\n  name:'bookInfo',\r\n  data() {\r\n    return {\r\n      publisher:[\r\n\r\n      ],\r\n      option1:[\r\n\r\n      ],\r\n      option2:[\r\n\r\n      ],\r\n\r\n      loading: true,\r\n      //   表格的数据\r\n      tableData: [\r\n        {\r\n        film:{\r\n          name:'',\r\n          id:'',\r\n          birthday:'',\r\n          time:'',\r\n          lang:'',\r\n          country:'',\r\n          brief:'',\r\n          score:'',\r\n          pic:'',\r\n        },\r\n        cate:[\r\n\r\n        ],\r\n        campany:[\r\n\r\n        ]\r\n        }\r\n   \r\n      ],\r\n      dialogFormVisible: false,\r\n      formLabelWidth: \"180px\",\r\n      // 设置form用于进行添加的时候绑定值\r\n      form: {\r\n        movie:{\r\n\r\n        },\r\n        id:\"\",\r\n        book_name:\"\",\r\n        price:\"\",\r\n        publisher:\"\",\r\n        date:\"\",\r\n        image:\"\",\r\n        big_class:\"\",\r\n        small_class:\"\",\r\n        author:\"\",\r\n        translator:\"\",\r\n      },\r\n      rules:{\r\n        book_id:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n        book_name:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n        price:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n        publisher:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n        date:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n        // image:[\r\n        //   { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        // ],\r\n        big_class:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n        small_class:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n        author:[\r\n          { required: true, message: \"请输入\", trigger: \"blur\" },\r\n        ],\r\n      },\r\n      currentIndex: \"\",\r\n      mark:0\r\n    };\r\n  },\r\n  created() {\r\n    //   设置回调函数，进行1.5秒的loading动画显示\r\n    setTimeout(() => {\r\n      this.loading = false;\r\n    }, 1500);\r\n   // this.getPublisher();\r\n    //this.getOption1();\r\n    this.getTableData();\r\n  },\r\n  watch:{\r\n    //2.x版本的bug 以前用1.x发现没有 假如现在是第三页，只有一条数据了。将其删除，就没有第三页了。应该跳到第二页展示出5条数据。\r\n    //可是数据没有展示。原因是获取list的时候page参数没有改变。依然是3\r\n    total(){\r\n      if(this.total==(this.page-1)*this.size&& this.total!=0){\r\n        this.page -= 1;\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    getOption1(){\r\n      axios.get(\"http://localhost:8083/getOption1\",{\r\n        headers: {\r\n          'Content-Type':'application/json; charset=UTF-8'//设置传输JSON格式即可\r\n        }\r\n      })\r\n        .then(resp=>  {\r\n          let code=resp.data.code;\r\n          if(code===200){\r\n            this.$data.option1=resp.data.data;\r\n          }\r\n          else {\r\n            this.$message({\r\n              type: \"fail\",\r\n              message: \"获取失败\"\r\n            });\r\n          }}\r\n        )\r\n    },\r\n    getOption2(big_class){\r\n      axios.post(\"http://localhost:8083/getOption2\",big_class,{\r\n        headers: {\r\n          'Content-Type':'application/json; charset=UTF-8'//设置传输JSON格式即可\r\n        }\r\n      })\r\n        .then(resp=>  {\r\n          let code=resp.data.code;\r\n          if(code===200){\r\n            this.$data.option2=resp.data.data;\r\n          }\r\n          else {\r\n            this.$message({\r\n              type: \"fail\",\r\n              message: \"获取失败\"\r\n            });\r\n          }}\r\n        )\r\n    },\r\n    getPublisher(){\r\n      axios.get(\"http://localhost:8083/getPublisher1\",{\r\n        headers: {\r\n          'Content-Type':'application/json; charset=UTF-8'//设置传输JSON格式即可\r\n        }\r\n      })\r\n        .then(resp=>  {\r\n          let code=resp.data.code;\r\n          if(code===200){\r\n            this.$data.publisher=resp.data.data;\r\n          }\r\n          else {\r\n            this.$message({\r\n              type: \"fail\",\r\n              message: \"获取失败\"\r\n            });\r\n          }}\r\n        )\r\n    },\r\n    getTableData(){\r\n\r\n    const _this = this;\r\n      axios\r\n        .get(\"http://localhost:8181/getfilm\")\r\n        .then(function (resp) {\r\n          console.log(resp.data);\r\n          _this.$data.tableData = resp.data;\r\n          console.log(_this.$data.tableData);\r\n          //var arr = []\r\n          //arr.push({name:resp.data.name,id:resp.data.id,birthday:resp.data.birthday,lang:resp.data.lang,country:resp.data.country,brief:resp.data.brief,score:resp.data.score,pic:resp.data.pic})\r\n          //this.$data.movies=arr;\r\n          //console.log(this.$data.movies);\r\n        });      \r\n    },\r\n        \r\n    \r\n    // 增加数据的方式，单独的设置一些值，用于增加功能，这些值放在对象里面进行设置，然后将这个新增的对象塞到总数据里面\r\n    add() {\r\n      this.form ={\r\n        book_id:\"\",\r\n        book_name:\"\",\r\n        price:\"\",\r\n        publisher:\"\",\r\n        date:\"\",\r\n        image:\"\",\r\n        big_class:\"\",\r\n        small_class:\"\",\r\n        author:\"\",\r\n        translator:\"\",\r\n      };\r\n      this.mark=2;\r\n      //   设置点击按钮之后进行显示对话框\r\n      this.dialogFormVisible = true;\r\n    },\r\n    update(formName) {\r\n      if(this.mark>1) this.mark=0;\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          axios.post(\"http://localhost:8083/updateBookInfo\",this.form,{\r\n            headers: {\r\n              'Content-Type':'application/json; charset=UTF-8'//设置传输JSON格式即可\r\n            }\r\n          })\r\n            .then(resp=>  {\r\n              let code=resp.data.code;\r\n              let data=resp.data.data;\r\n              if(code===200){\r\n                //   this.form.date = reformat(this.form.date);\r\n                //    可以在html上面进行设置日期的格式化\r\n                //   将我们添加的信息提交到总数据里面\r\n                if(data==='insert') this.tableData.push(this.form);\r\n                else{\r\n                  this.form.book_id=data.book_id;\r\n                  this.form.image=data.image;\r\n                }\r\n                this.dialogFormVisible = false;\r\n                this.$message({\r\n                  type: \"success\",\r\n                  message: \"更新成功\"\r\n                });\r\n              }\r\n              else {\r\n                this.$message({\r\n                  type: \"fail\",\r\n                  message: \"更新失败\"\r\n                });\r\n              }}\r\n            );}\r\n      })\r\n    },\r\n    handleEdit(index, row) {\r\n      if(this.mark>1) this.mark=0;\r\n      // 将数据的index传递过来用于实现数据的回显\r\n      this.form = this.tableData[index+(this.page-1)*this.size];\r\n      // 设置对话框的可见\r\n      this.dialogFormVisible = true;\r\n    },\r\n    handleDelete(index, row) {\r\n      // 设置类似于console类型的功能\r\n      this.$confirm(\"永久删除, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      })\r\n        .then(() => {\r\n          axios.post(\"http://localhost:8083/deleteBookInfo\",row,{\r\n            headers: {\r\n              'Content-Type':'application/json; charset=UTF-8'//设置传输JSON格式即可\r\n            }\r\n          })\r\n            .then(resp=>  {\r\n              let code=resp.data.code;\r\n              if(code===200){\r\n                // 移除对应索引位置的数据，可以对row进行设置向后台请求删除数据\r\n                this.tableData.splice(index+(this.page-1)*this.size, 1);\r\n                this.$message({\r\n                  type: \"success\",\r\n                  message: \"删除成功\"\r\n                });\r\n              }\r\n              else {\r\n                this.$message({\r\n                  type: \"fail\",\r\n                  message: \"删除失败\"\r\n                });\r\n              }}\r\n            )\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\"\r\n          });\r\n        });\r\n    },\r\n    cancel() {\r\n      if(this.mark>1) this.mark=0;\r\n      // 取消的时候直接设置对话框不可见即可\r\n      this.dialogFormVisible = false;\r\n    },\r\n    handleSizeChange(val){\r\n      this.size = val;\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.page = val;\r\n    },\r\n    handleAvatarSuccess(res, file) {\r\n      this.$message({\r\n        type: \"success\",\r\n        message: \"图片上传成功,确认后刷新可见\"\r\n      })\r\n      // this.form.image = URL.createObjectURL(file.raw);\r\n      // console.log(this.form.image);\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      // const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      // if (!isJPG) {\r\n      //   this.$message.error('上传头像图片只能是 JPG 格式!');\r\n      // isJPG &&}\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 2MB!');\r\n      }\r\n      return  isLt2M;\r\n    },\r\n    handleAvatarFail(file, fileList){\r\n      console.log(\"文件已被移动\")\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style >\r\n\r\n</style>\r\n\r\n"],"sourceRoot":"src/components"}]}